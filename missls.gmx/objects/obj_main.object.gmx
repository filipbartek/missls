var wview_desired = pos_x_max + margin - view_xview[VIEW_MAIN];
var hview_desired = pos_y_max + margin - view_yview[VIEW_MAIN];

if (wview_desired &gt; hview_desired * aspect_ratio) {
    hview_desired = wview_desired / aspect_ratio;
} else {
